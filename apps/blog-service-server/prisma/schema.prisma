datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model BlogPost {
  createdAt  DateTime  @default(now())
  date       DateTime?
  id         String    @id @default(cuid())
  shareCount Int?
  text       String?
  title      String?
  updatedAt  DateTime  @updatedAt
  viewCount  Int?
}
